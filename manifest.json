{
  "name": "@deskpro-apps/asset-panda",
  "title": "AssetPanda",
  "description": "",
  "version": "0.0.6",
  "scope": "agent",
  "isSingleInstall": false,
  "hasDevMode": true,
  "targets": [{ "target": "user_sidebar", "entrypoint": "index.html" }],
  "serveUrl": "https://apps-cdn.deskpro-service.com/__name__/__version__",
  "entityAssociations": {
    "linkedAssetPanda": {
      "entity": "person",
      "type": "external_id",
      "title": "Linked AssetPanda Stuff"
    }
  },
  "settings": {
    "email": {
      "title": "Email",
      "type": "string",
      "isRequired": true,
      "isBackendOnly": true,
      "validationPattern": "^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$",
      "order": 10
    },
    "password": {
      "title": "Password",
      "type": "string",
      "isRequired": true,
      "isBackendOnly": true,
      "order": 20
    },
    "auth": {
      "title": "",
      "type": "app_embedded",
      "options": { "entrypoint": "#/admin/global-auth" },
      "isRequired": false,
      "isBackendOnly": false,
      "order": 30
    }
  },
  "proxy": {
    "whitelist": [
      {
        "url": "https://api.assetpanda.com/v3/.*",
        "methods": ["GET", "POST"],
        "timeout": 20
      }
    ]
  }
}
